/*
1. For long integer or double in scan use LF in small
2. If we put a 4.5 at a time then first one will be 4= x and 0.5 =y
because point can't be portion of integer 
 */

#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
int main() {
    int x = 0;
    double y = 0;
    char c = 0;
    char name[50];

    scanf("%d %lf",&x,&y);
    printf("x=%d, y=%lf",x,y);

    scanf("%s", name);

    printf("name: %s", name);

    return 0;
}
