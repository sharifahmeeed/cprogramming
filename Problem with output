#include <stdio.h>
#include <stdlib.h>

int main(){
    printf("Enter two number: ");
    int i[]={};
    i[1]=0, i[2]=0;
    scanf("%d%d",&i[1],&i[2]);
   // fflush(stdin);
    //scanf("%d",&i[2]);

    printf("\ni[1]= %d i[2]=%d\n",i[1],i[2]);


    int i1 = i[1];
    int i2 = i[2];

    printf("i1=%d i2=%d\n", i1,i2);

    if(i[1]>i[2])
        printf("If Max:%d",i[1]);
    else
        printf("If Max:%d",i[2]);

    printf("\nMax array: %d",((i[1]>i[2])? i[1]:i[2]));
    printf("\nMax interger: %d",((i1>i2)? i1:i2));


    int gd=5;
    printf("\n%d",gd);



    return 0;
}
